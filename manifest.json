{
  "name": "Hello Extensions",
  "description": "Base Level Extension",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "background": {
    "scripts": ["background.js"]},
  "permissions": [
    "tabs",
    "https://bodhi.carma.com/*",
    "https://bodhi-staging.carma.com/*"
  
  ]
}